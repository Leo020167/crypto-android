<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".module.home.SelectAccountActivity">

    <include layout="@layout/toolbar" />


    <TextView
        android:id="@+id/txt_content10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:text="@string/shuzihuobigendanzhanghu"
        android:textColor="@color/c1d3155"
        android:textSize="@dimen/font_size_16"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/cd8d7dc" />

    <TextView
        android:id="@+id/txt_content20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:text="@string/guojiqihuogendanzhanghu"
        android:textColor="@color/c1d3155"
        android:textSize="@dimen/font_size_16" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/cd8d7dc" />

    <TextView
        android:id="@+id/txt_content30"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:text="@string/waihuigendanzhanghu"
        android:textColor="@color/c1d3155"
        android:textSize="@dimen/font_size_16"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/cd8d7dc" />
</LinearLayout>