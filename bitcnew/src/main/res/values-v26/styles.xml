<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppThemeToolBar.Translucent" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:textColorPrimary">@color/actionbar_light_title_color</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--<item user_name="actionBarSize">@dimen/actionBarSize</item>-->
        <item name="statusBarDarkFont">true</item>
        <item name="toolbar_textColor">@color/black</item>
        <item name="toolbar_elevation">0.5dp</item>
        <item name="toolbar_lineHeight">0.5dp</item>
    </style>
    <style name="AppWelcome.style" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Theme.Taojinroad.ActionBar.logout" parent="Theme.AppCompat.Light">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>